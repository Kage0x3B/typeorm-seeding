{
    "name": "@kage0x3b/typeorm-seeding",
    "version": "0.1.1",
    "description": "Factory and seeder library for TypeORM with relationship descriptors, variants, and automatic cleanup",
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "build": "tsdown",
        "watch": "tsdown --watch",
        "prepare": "pnpm build",
        "test": "vitest run",
        "typecheck": "tsc --noEmit",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
        "preinstall": "npx only-allow pnpm",
        "prepublishOnly": "pnpm test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Kage0x3B/typeorm-seeding.git"
    },
    "author": {
        "name": "Moritz Hein",
        "email": "moritz.hein@live.de",
        "url": "https://moritz.website"
    },
    "keywords": [
        "typeorm",
        "factory",
        "seeder",
        "seeding",
        "fixtures",
        "testing",
        "faker",
        "typescript",
        "database"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Kage0x3B/typeorm-seeding/issues"
    },
    "homepage": "https://github.com/Kage0x3B/typeorm-seeding#readme",
    "devDependencies": {
        "@arethetypeswrong/core": "^0.18.2",
        "@swc/core": "^1.15.11",
        "@types/node": "^24.10.13",
        "@vitest/coverage-v8": "^4.0.18",
        "better-sqlite3": "^12.6.2",
        "prettier": "^3.8.1",
        "publint": "^0.3.17",
        "reflect-metadata": "^0.2.2",
        "tsdown": "^0.20.3",
        "typescript": "^5.9.3",
        "unplugin-swc": "^1.5.9",
        "vitest": "^4.0.18"
    },
    "dependencies": {
        "@faker-js/faker": "^10.3.0"
    },
    "peerDependencies": {
        "reflect-metadata": "^0.2.0",
        "typeorm": "^0.3.20"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "files": [
        "dist/**/*"
    ],
    "types": "./dist/index.d.mts",
    "exports": {
        ".": "./dist/index.mjs",
        "./package.json": "./package.json"
    },
    "publishConfig": {
        "access": "public"
    }
}
